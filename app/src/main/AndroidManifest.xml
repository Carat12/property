<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.property">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/house"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/house"
        android:supportsRtl="true"
        android:theme="@style/Theme.Property.Rose">
        <activity android:name=".ui.document.AddDocActivity" />
        <activity android:name=".ui.document.DocActivity" />
        <activity android:name=".ui.tenant.AddTenantActivity" />
        <activity android:name=".ui.tenant.TenantActivity" />

        <service
            android:name=".ui.property.addproperty.GetAddressIntentService"
            android:exported="false" />

        <activity android:name=".ui.property.addproperty.AddPropertyActivity" />
        <activity android:name=".ui.property.PropertyActivity" />
        <activity android:name=".ui.auth.login.LoginActivity" />
        <activity android:name=".ui.auth.register.RegisterActivity" />
        <activity android:name=".ui.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.HomeActivity" />
    </application>

</manifest>